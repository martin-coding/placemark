{{#each locations}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{title}}
      <span style="font-size: 0.5em; color: grey;">({{latitude}},{{longitude}})</span>
    </h2>
    <p>{{description}}</p>
    <br />
    <a href="/dashboard/deletelocation/{{_id}}" class="button">
      <i class="fas fa-trash"></i>
    </a>
  </div>
{{/each}}